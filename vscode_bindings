[
        // https://stackoverflow.com/questions/38957302/is-there-a-quick-change-tabs-function-in-visual-studio-code
        {
            "key": "ctrl+tab",
            "command": "workbench.action.nextEditorInGroup"
        },
        {
            "key": "ctrl+shift+tab",
            "command": "workbench.action.previousEditorInGroup"
        },
        {
            "key": "ctrl+l",
            "command": "workbench.action.nextEditorInGroup"
        },
        {
            "key": "ctrl+h",
            "command": "workbench.action.previousEditorInGroup"
        },
        { "key": "ctrl+shift+j", "command": "workbench.action.terminal.focusNext" },
        { "key": "ctrl+shift+k", "command": "workbench.action.terminal.focusPrevious" },
        { "key": "ctrl+shift+q", "command": "workbench.action.terminal.kill" }
]
